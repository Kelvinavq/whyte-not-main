{
    "crons": [
      {
        "path": "/api/cron",
        "schedule": "0 10 * * *"
      }
    ],
    "routes": [
      {
        "src": "/php/process.php", 
        "methods": ["POST"], 
        "dest": "/php/process.php" 
      },
      {
        "src": "/api/form", 
        "methods": ["POST"], 
        "dest": "/src/components/form/fom.tsx" 
      }
    ]
  }
  